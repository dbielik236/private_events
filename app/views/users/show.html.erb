<h1><%= current_user.email %>, these are the events you've created</h1>


  <% current_user.created_events.each do |event| %>
    <li><%= event.title %></li>
    <br>
  <% end %>

<h1>These are the events you're attending</h1>
<% current_user.enrollments.each do |enrollment| %>
    <li><%= Event.find(enrollment.attended_event_id).title %></li>
    <br>
  <% end %>

  